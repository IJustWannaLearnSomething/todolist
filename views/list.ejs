<%- include("header") -%>

<div class="box">
  <% for(let i = 0; i < newItems.length ; i++) { %>
  <div class="item">
    <input type="checkbox">
    <p> <%= newItems[i] %> </p>
  </div>
  <% } %>
  <form class="item" action="/" method="post">
    <input type="text" name="userInput" placeholder="New Item" autocomplete="off">
    <button type="submit" name="list" value=<%= titleName %>>+</button>
  </form>

</div>

<%- include("footer") -%>